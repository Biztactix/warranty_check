{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Device42": {
    "BaseUrl": "https://your-device42-instance.com",
    "Username": "your-username",
    "Password": "your-password",
    "VerifySsl": true,
    "TimeoutSeconds": 30
  },
  "Discovery": {
    "Cisco": false,
    "Dell": false,
    "HP": false,
    "IBM": false,
    "Lenovo": false,
    "Meraki": false,
    "ForceUpdate": false,
    "BatchSize": 50
  },
  "Vendors": {
    "Cisco": {
      "Enabled": false,
      "Url": "https://api.cisco.com/sn2info/v2/coverage/summary/serial_numbers",
      "ClientId": "your-cisco-client-id",
      "ClientSecret": "your-cisco-client-secret",
      "TimeoutSeconds": 30,
      "RetryCount": 3,
      "BatchSize": 70
    },
    "Dell": {
      "Enabled": false,
      "Url": "https://apigtwb2c.us.dell.com/PROD/sbil/eapi/v5/asset-entitlements",
      "ClientId": "your-dell-client-id",
      "ClientSecret": "your-dell-client-secret",
      "TimeoutSeconds": 30,
      "RetryCount": 3,
      "BatchSize": 50
    },
    "HP": {
      "Enabled": false,
      "Url": "https://css.api.hp.com/omaha/v3/product",
      "ApiKey": "your-hp-api-key",
      "TimeoutSeconds": 30,
      "RetryCount": 3,
      "BatchSize": 50
    },
    "IBM": {
      "Enabled": false,
      "Url": "https://www-947.ibm.com/support/servicerequest/Home.action",
      "TimeoutSeconds": 30,
      "RetryCount": 3,
      "BatchSize": 50
    },
    "Lenovo": {
      "Enabled": false,
      "Url": "https://pcsupport.lenovo.com/us/en/api/v4/upsell/redport/getIbaseInfo",
      "TimeoutSeconds": 30,
      "RetryCount": 3,
      "BatchSize": 50
    },
    "Meraki": {
      "Enabled": false,
      "Url": "https://api.meraki.com/api/v1",
      "ApiKey": "your-meraki-api-key",
      "TimeoutSeconds": 30,
      "RetryCount": 3,
      "BatchSize": 50
    }
  },
  "General": {
    "LogLevel": "Information",
    "EnableTelemetry": true,
    "PauseBetweenCalls": 1000
  }
}